{
  "name": "unit_testing",
  "version": "1.0.0",
  "description": "atm-with-js program",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "echo \"Dummy build script\"",
    "test:junit": "cross-env MOCHA_FILE=junit.xml mocha test --reporter mocha-junit-reporter",
    "test:xunit": "cross-env MOCHA_FILE=xunit.xml mocha test --reporter mocha-xunit-reporter",
    "test:report": "mocha --reporter mochawesome",
    "coverage": "c8 npm run test:dev",
    "test": "npm run test:junit",
    "test:dev": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@autocode.git.epam.com:dzmitry_shaplyka/atm-with-js.git"
  },
  "authors": [
    "lena_stepanyan@epam.com"
  ],
  "license": "EPAM Systems, Inc. All Rights Reserved.",
  "engines": {
    "node": "^22"
  },
  "dependencies": {
    "chai": "^4.5.0",
    "cross-env": "7.0.3",
    "mocha": "^11.7.1",
    "mocha-junit-reporter": "2.2.1",
    "mocha-xunit-reporter": "2.3.0",
    "mochawesome": "^7.1.3"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "eslint": "^8.57.1"
  }
}
